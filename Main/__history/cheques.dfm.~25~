  object Qcheques: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO control_cheques'
      
        '  (chequeid, facturaid, tipo_moneda, importe_mn, importe_cuc, fe' +
        'cha_recepcion, registrado)'
      'VALUES'
      
        '  (:chequeid, :facturaid, :tipo_moneda, :importe_mn, :importe_cu' +
        'c, :fecha_recepcion, :registrado)')
    SQLDelete.Strings = (
      'DELETE FROM control_cheques'
      'WHERE'
      '  chequeid = :Old_chequeid AND facturaid = :Old_facturaid')
    SQLUpdate.Strings = (
      'UPDATE control_cheques'
      'SET'
      
        '  chequeid = :chequeid, facturaid = :facturaid, tipo_moneda = :t' +
        'ipo_moneda, importe_mn = :importe_mn, importe_cuc = :importe_cuc' +
        ', fecha_recepcion = :fecha_recepcion, registrado = :registrado'
      'WHERE'
      '  chequeid = :Old_chequeid AND facturaid = :Old_facturaid')
    SQLRefresh.Strings = (
      
        'SELECT chequeid, facturaid, tipo_moneda, importe_mn, importe_cuc' +
        ', fecha_recepcion, registrado FROM control_cheques'
      'WHERE'
      '  chequeid = :chequeid AND facturaid = :facturaid')
    Connection = Fmain.UniConnection1
