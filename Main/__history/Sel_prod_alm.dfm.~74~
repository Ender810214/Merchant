  object UniQuery1: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO ubicacion'
      
        '  (almacenid, ubicacionid, contratoid, clienteid, productoid, ex' +
        'istencia, precio_cup, precio_cuc, precio_usd, codigo_producto, d' +
        'escripcion_producto)'
      'VALUES'
      
        '  (:almacenid, :ubicacionid, :contratoid, :clienteid, :productoi' +
        'd, :existencia, :precio_cup, :precio_cuc, :precio_usd, :codigo_p' +
        'roducto, :descripcion_producto)')
    SQLDelete.Strings = (
      'DELETE FROM ubicacion'
      'WHERE'
      
        '  almacenid = :Old_almacenid AND ubicacionid = :Old_ubicacionid ' +
        'AND contratoid = :Old_contratoid AND clienteid = :Old_clienteid ' +
        'AND productoid = :Old_productoid')
    SQLUpdate.Strings = (
      'UPDATE ubicacion'
      'SET'
      
        '  almacenid = :almacenid, ubicacionid = :ubicacionid, contratoid' +
        ' = :contratoid, clienteid = :clienteid, productoid = :productoid' +
        ', existencia = :existencia, precio_cup = :precio_cup, precio_cuc' +
        ' = :precio_cuc, precio_usd = :precio_usd, codigo_producto = :cod' +
        'igo_producto, descripcion_producto = :descripcion_producto'
      'WHERE'
      
        '  almacenid = :Old_almacenid AND ubicacionid = :Old_ubicacionid ' +
        'AND contratoid = :Old_contratoid AND clienteid = :Old_clienteid ' +
        'AND productoid = :Old_productoid')
    SQLRefresh.Strings = (
      
        'SELECT almacenid, ubicacionid, contratoid, clienteid, productoid' +
        ', existencia, precio_cup, precio_cuc, precio_usd, codigo_product' +
        'o, descripcion_producto FROM ubicacion'
      'WHERE'
      
        '  almacenid = :almacenid AND ubicacionid = :ubicacionid AND cont' +
        'ratoid = :contratoid AND clienteid = :clienteid AND productoid =' +
        ' :productoid')
    Connection = Fmain.UniConnection1
    SQL.Strings = (
      
        'select ubicacionid, existencia, contratoid,clienteid,codcontrato' +
        ',codcliente, productoid,almacenid,precio_cup,'
      
        'codigo_producto,descripcion_producto, precio_usd,anexo,posicion_' +
        'anexo,costo_unitario, reservado, ueb from ubicacion where contra' +
        'toid=:contrato and clienteid=:cliente and existencia>0'
      '')
    Left = 56
    Top = 128
    ParamData = <
      item
        DataType = ftInteger
        Name = 'contrato'
        Value = nil
      end
      item
        DataType = ftInteger
        Name = 'cliente'
        Value = nil
      end>
