{$R *.dfm}

procedure TFsel_contrato.AdvSmoothButton1Click(Sender: TObject);
begin
  fanexos.UniQuery1.ParamByName('contratoid').Value:=fmain.QContratoscontratoid.Value;
  fanexos.UniQuery1.ExecSQL;
  fanexos.UniQuery1.open;
  fmain.Qanexoscontratoid.Value:=Fmain.QContratoscontratoid.Value;
  Fmain.Qanexosclienteid.Value:=Fmain.QContratosclienteid.Value;
  close;
end;

procedure TFsel_contrato.AdvSmoothButton2Click(Sender: TObject);
begin
Close;
end;

end.
