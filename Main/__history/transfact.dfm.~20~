  object Qtrans_fac: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO transp_fac'
      '  (facturaid, transpid)'
      'VALUES'
      '  (:facturaid, :transpid)')
    SQLDelete.Strings = (
      'DELETE FROM transp_fac'
      'WHERE'
      '  facturaid = :Old_facturaid AND transpid = :Old_transpid')
    SQLUpdate.Strings = (
      'UPDATE transp_fac'
      'SET'
      '  facturaid = :facturaid, transpid = :transpid'
      'WHERE'
      '  facturaid = :Old_facturaid AND transpid = :Old_transpid')
    SQLRefresh.Strings = (
      'SELECT facturaid, transpid FROM transp_fac'
      'WHERE'
      '  facturaid = :facturaid AND transpid = :transpid')
    Connection = Fmain.UniConnection1
    SQL.Strings = (
      'select * from transp_fac')
