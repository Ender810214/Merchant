  object Qcontrol711: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO control_doc'
      
        '  (id_doc, descripcion, facturaid, importemn, importecuc, fecha_' +
        'aprobacion, tipo_compra, unidadid, tipo_doc, proveedor, registra' +
        'do)'
      'VALUES'
      
        '  (:id_doc, :descripcion, :facturaid, :importemn, :importecuc, :' +
        'fecha_aprobacion, :tipo_compra, :unidadid, :tipo_doc, :proveedor' +
        ', :registrado)')
    SQLDelete.Strings = (
      'DELETE FROM control_doc'
      'WHERE'
      
        '  id_doc = :Old_id_doc AND unidadid = :Old_unidadid AND tipo_doc' +
        ' = :Old_tipo_doc')
    SQLUpdate.Strings = (
      'UPDATE control_doc'
      'SET'
      
        '  id_doc = :id_doc, descripcion = :descripcion, facturaid = :fac' +
        'turaid, importemn = :importemn, importecuc = :importecuc, fecha_' +
        'aprobacion = :fecha_aprobacion, tipo_compra = :tipo_compra, unid' +
        'adid = :unidadid, tipo_doc = :tipo_doc, proveedor = :proveedor, ' +
        'registrado = :registrado'
      'WHERE'
      
        '  id_doc = :Old_id_doc AND unidadid = :Old_unidadid AND tipo_doc' +
        ' = :Old_tipo_doc')
    SQLRefresh.Strings = (
      
        'SELECT id_doc, descripcion, facturaid, importemn, importecuc, fe' +
        'cha_aprobacion, tipo_compra, unidadid, tipo_doc, proveedor, regi' +
        'strado FROM control_doc'
      'WHERE'
      
        '  id_doc = :id_doc AND unidadid = :unidadid AND tipo_doc = :tipo' +
        '_doc')
    Connection = Fmain.UniConnection1
    SQL.Strings = (
      'select * from control_doc')
    RefreshOptions = [roAfterInsert, roAfterUpdate]
    Active = True
    Left = 120
    Top = 344
