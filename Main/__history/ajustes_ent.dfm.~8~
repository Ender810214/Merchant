  object Qprod_ent: TUniQuery
    SQLUpdate.Strings = (
      'UPDATE prod_entradas'
      'SET'
      
        '  precio_cup = :precio_cup, precio_cuc = :precio_cuc, precio_usd' +
        ' = :precio_usd, costo_unitario = :costo_unitario'
      'WHERE'
      
        '  entradaid = :Old_entradaid AND productoid = :Old_productoid AN' +
        'D costo_total = :Old_costo_total AND anexoid = :Old_anexoid AND ' +
        'cod_contrato = :Old_cod_contrato AND cod_cliente = :Old_cod_clie' +
        'nte AND posicion_anexo = :Old_posicion_anexo')
    SQLRefresh.Strings = (
      
        'SELECT entradaid, productoid, cantidad, precio_cup, precio_cuc, ' +
        'precio_usd, costo_unitario, costo_total, importe_cuc, importe_mn' +
        ', clienteid, contratoid, anexoid, plazoid, cod_contrato, cod_cli' +
        'ente, cod_producto, des_producto, precio_total, posicion_anexo F' +
        'ROM prod_entradas'
      'WHERE'
      
        '  entradaid = :entradaid AND productoid = :productoid AND costo_' +
        'total = :costo_total AND anexoid = :anexoid AND cod_contrato = :' +
        'cod_contrato AND cod_cliente = :cod_cliente AND posicion_anexo =' +
        ' :posicion_anexo')
    Connection = Fmain.UniConnection1
    SQL.Strings = (
      'select * from prod_entradas')
    MasterSource = DataSource1
    MasterFields = 'entradaid'
    DetailFields = 'entradaid'
    Left = 480
    Top = 312
