  object Qfacturador_fac: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO facturador_fact'
      '  (facturaid, facturadorid)'
      'VALUES'
      '  (:facturaid, :facturadorid)')
    SQLDelete.Strings = (
      'DELETE FROM facturador_fact'
      'WHERE'
      
        '  facturaid = :Old_facturaid AND facturadorid = :Old_facturadori' +
        'd')
    SQLUpdate.Strings = (
      'UPDATE facturador_fact'
      'SET'
      '  facturaid = :facturaid, facturadorid = :facturadorid'
      'WHERE'
      
        '  facturaid = :Old_facturaid AND facturadorid = :Old_facturadori' +
        'd')
    SQLRefresh.Strings = (
      'SELECT facturaid, facturadorid FROM facturador_fact'
      'WHERE'
      '  facturaid = :facturaid AND facturadorid = :facturadorid')
    Connection = Fmain.UniConnection1
    SQL.Strings = (
      'select * from facturador_fact')
