{$R *.dfm}
 uses main, transferencias;
procedure TFsel_plazo_transf.AdvSmoothButton1Click(Sender: TObject);
begin
 {FTransferencias.Qtransfcontratof.Value:=Qplazoscontratoid.Value;
  FTransferencias.Qtransfclientef.Value:=Qplazosclienteid.Value;
FTransferencias.Qtransfubicacionf.Value:=Qubicacionesubicacionid.Value;
FTransferencias.Qtransfproductof.Value:=Qubicacionesproductoid.Value;
FTransferencias.Qtransfcodcontratof.Value:=Qubicacionescodcontrato.Value;
FTransferencias.Qtransfcodclientef.Value:=Qubicacionescodcliente.Value;
FTransferencias.Qtransfcodprodf.Value:=Qubicacionescodigo_producto.Value;}
end;

end.
