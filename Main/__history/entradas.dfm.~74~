  object Qentradas: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO entradas'
      '  (almacenid, no_vale, fecha_entrada, estado, tipovale)'
      'VALUES'
      '  ( :almacenid, :no_vale, :fecha_entrada, :estado, :tipovale)')
    SQLDelete.Strings = (
      'DELETE FROM entradas'
      'WHERE'
      '  entradaid = :Old_entradaid AND almacenid = :Old_almacenid')
    SQLUpdate.Strings = (
      'UPDATE entradas'
      'SET'
      
        '  entradaid = :entradaid, almacenid = :almacenid, no_vale = :no_' +
        'vale, fecha_entrada = :fecha_entrada, estado = :estado, tipovale' +
        ' = :tipovale'
      'WHERE'
      '  entradaid = :Old_entradaid AND almacenid = :Old_almacenid')
    SQLRefresh.Strings = (
      
        'SELECT entradaid, almacenid, no_vale, fecha_entrada, estado, tip' +
        'ovale FROM entradas'
      'WHERE'
      '  entradaid = :entradaid AND almacenid = :almacenid')
    Connection = Fmain.UniConnection1
    SQL.Strings = (
      'select * from entradas')
    BeforePost = QentradasBeforePost
