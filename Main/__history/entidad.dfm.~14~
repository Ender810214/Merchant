  object QEntidades: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO entidad'
      
        '  (codigo, nombre, direccion, nit, cuenta_cuc, cuenta_mn, sucurs' +
        'al_cuc, sucursal_mn)'
      'VALUES'
      
        '  (:codigo, :nombre, :direccion, :nit, :cuenta_cuc, :cuenta_mn, ' +
        ':sucursal_cuc, :sucursal_mn)')
    SQLDelete.Strings = (
      'DELETE FROM entidad'
      'WHERE'
      '  entidadid = :Old_entidadid AND codigo = :Old_codigo')
    SQLUpdate.Strings = (
      'UPDATE entidad'
      'SET'
      
        '  codigo = :codigo, nombre = :nombre, direccion = :direccion, ni' +
        't = :nit, cuenta_cuc = :cuenta_cuc, cuenta_mn = :cuenta_mn, sucu' +
        'rsal_cuc = :sucursal_cuc, sucursal_mn = :sucursal_mn'
      'WHERE'
      '  entidadid = :Old_entidadid AND codigo = :Old_codigo')
    SQLRefresh.Strings = (
      
        'SELECT entidadid, codigo, nombre, direccion, nit, cuenta_cuc, cu' +
        'enta_mn, sucursal_cuc, sucursal_mn FROM entidad'
      'WHERE'
      '  entidadid = :entidadid AND codigo = :codigo')
    Connection = Fmain.UniConnection1
    SQL.Strings = (
      'select * from entidad')
    RefreshOptions = [roAfterUpdate]
    BeforePost = QEntidadesBeforePost
