  object UniQuery1: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO plazos_entrega'
      
        '  (contratoid, clienteid, anexoid, productoid, plazoid, cantidad' +
        '_contratada, fecha_entrega)'
      'VALUES'
      
        '  (:contratoid, :clienteid, :anexoid, :productoid, :plazoid, :ca' +
        'ntidad_contratada, :fecha_entrega)')
    SQLDelete.Strings = (
      'DELETE FROM plazos_entrega'
      'WHERE'
      
        '  contratoid = :Old_contratoid AND clienteid = :Old_clienteid AN' +
        'D anexoid = :Old_anexoid AND productoid = :Old_productoid AND pl' +
        'azoid = :Old_plazoid')
    SQLUpdate.Strings = (
      'UPDATE plazos_entrega'
      'SET'
      
        '  contratoid = :contratoid, clienteid = :clienteid, anexoid = :a' +
        'nexoid, productoid = :productoid, plazoid = :plazoid, cantidad_c' +
        'ontratada = :cantidad_contratada, fecha_entrega = :fecha_entrega'
      'WHERE'
      
        '  contratoid = :Old_contratoid AND clienteid = :Old_clienteid AN' +
        'D anexoid = :Old_anexoid AND productoid = :Old_productoid AND pl' +
        'azoid = :Old_plazoid')
    SQLRefresh.Strings = (
      
        'SELECT contratoid, clienteid, anexoid, productoid, plazoid, cant' +
        'idad_contratada, fecha_entrega FROM plazos_entrega'
      'WHERE'
      
        '  contratoid = :contratoid AND clienteid = :clienteid AND anexoi' +
        'd = :anexoid AND productoid = :productoid AND plazoid = :plazoid')
