  object QProd_Entradas: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO prod_entradas'
      
        '  (entradaid, productoid, cantidad, precio_cup, precio_cuc, prec' +
        'io_usd, costo_unitario, costo_total, importe_cuc, importe_mn, cl' +
        'ienteid, contratoid, anexoid, plazoid, cod_contrato, cod_cliente' +
        ', cod_producto, des_producto, precio_total, posicion_anexo)'
      'VALUES'
      
        '  (:entradaid, :productoid, :cantidad, :precio_cup, :precio_cuc,' +
        ' :precio_usd, :costo_unitario, :costo_total, :importe_cuc, :impo' +
        'rte_mn, :clienteid, :contratoid, :anexoid, :plazoid, :cod_contra' +
        'to, :cod_cliente, :cod_producto, :des_producto, :precio_total, :' +
        'posicion_anexo)')
    SQLDelete.Strings = (
      'DELETE FROM prod_entradas'
      'WHERE'
      
        '  entradaid = :Old_entradaid AND productoid = :Old_productoid AN' +
        'D costo_total = :Old_costo_total AND anexoid = :Old_anexoid AND ' +
        'cod_contrato = :Old_cod_contrato AND cod_cliente = :Old_cod_clie' +
        'nte AND posicion_anexo = :Old_posicion_anexo')
    SQLUpdate.Strings = (
      'UPDATE prod_entradas'
      'SET'
      
        '  entradaid = :entradaid, productoid = :productoid, cantidad = :' +
        'cantidad, precio_cup = :precio_cup, precio_cuc = :precio_cuc, pr' +
        'ecio_usd = :precio_usd, costo_unitario = :costo_unitario, costo_' +
        'total = :costo_total, importe_cuc = :importe_cuc, importe_mn = :' +
        'importe_mn, clienteid = :clienteid, contratoid = :contratoid, an' +
        'exoid = :anexoid, plazoid = :plazoid, cod_contrato = :cod_contra' +
        'to, cod_cliente = :cod_cliente, cod_producto = :cod_producto, de' +
        's_producto = :des_producto, precio_total = :precio_total, posici' +
        'on_anexo = :posicion_anexo'
      'WHERE'
      
        '  entradaid = :Old_entradaid AND productoid = :Old_productoid AN' +
        'D costo_total = :Old_costo_total AND anexoid = :Old_anexoid AND ' +
        'cod_contrato = :Old_cod_contrato AND cod_cliente = :Old_cod_clie' +
        'nte AND posicion_anexo = :Old_posicion_anexo')
    SQLRefresh.Strings = (
      
        'SELECT entradaid, productoid, cantidad, precio_cup, precio_cuc, ' +
        'precio_usd, costo_unitario, costo_total, importe_cuc, importe_mn' +
        ', clienteid, contratoid, anexoid, plazoid, cod_contrato, cod_cli' +
        'ente, cod_producto, des_producto, precio_total, posicion_anexo F' +
        'ROM prod_entradas'
      'WHERE'
      
        '  entradaid = :entradaid AND productoid = :productoid AND costo_' +
        'total = :costo_total AND anexoid = :anexoid AND cod_contrato = :' +
        'cod_contrato AND cod_cliente = :cod_cliente AND posicion_anexo =' +
        ' :posicion_anexo')
    Connection = Fmain.UniConnection1
    SQL.Strings = (
      'select * from prod_entradas where entradaid=:ventradaid'
      '')
    Active = True
    AfterInsert = QProd_EntradasAfterInsert
    BeforePost = QProd_EntradasBeforePost
    Left = 296
    Top = 200
