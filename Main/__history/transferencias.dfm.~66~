  object Qtransf: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO tranf_contratos'
      '  ( ubicacionf, plazoid, cantidad_transf, estado, fecha)'
      'VALUES'
      '  (:ubicacionf, :plazoid, :cantidad_transf, :estado, :fecha)')
    SQLDelete.Strings = (
      'DELETE FROM tranf_contratos'
      'WHERE'
      '  transferenciaid = :Old_transferenciaid')
    SQLUpdate.Strings = (
      'UPDATE tranf_contratos'
      'SET'
      
        '  transferenciaid = :transferenciaid, ubicacionf = :ubicacionf, ' +
        'plazoid = :plazoid, cantidad_transf = :cantidad_transf, estado =' +
        ' :estado, fecha = :fecha'
      'WHERE'
      '  transferenciaid = :Old_transferenciaid')
    SQLRefresh.Strings = (
      
        'SELECT transferenciaid, ubicacionf, plazoid, cantidad_transf, es' +
        'tado, fecha FROM tranf_contratos'
      'WHERE'
      '  transferenciaid = :transferenciaid')
    Connection = Fmain.UniConnection1
    SQL.Strings = (
      
        'select * from tranf_contratos where fecha between :infecha1 and ' +
        ':infecha2')
    AfterCancel = QtransfAfterCancel
    AfterScroll = QtransfAfterScroll
    Left = 456
    Top = 496
    ParamData = <
      item
        DataType = ftDate
        Name = 'infecha1'
        Value = nil
      end
      item
        DataType = ftDate
        Name = 'infecha2'
        Value = nil
      end>
