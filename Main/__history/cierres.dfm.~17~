  object QCierres: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO cierres'
      '  ( fecha1, fecha2, "Estado", descripcion)'
      'VALUES'
      '  ( :fecha1, :fecha2, :"Estado", :descripcion)')
    SQLDelete.Strings = (
      'DELETE FROM cierres'
      'WHERE'
      '  cierreid = :Old_cierreid')
    SQLUpdate.Strings = (
      'UPDATE cierres'
      'SET'
      
        '  cierreid = :cierreid, fecha1 = :fecha1, fecha2 = :fecha2, "Est' +
        'ado" = :"Estado", descripcion = :descripcion'
      'WHERE'
      '  cierreid = :Old_cierreid')
    SQLRefresh.Strings = (
      
        'SELECT cierreid, fecha1, fecha2, "Estado", descripcion FROM cier' +
        'res'
