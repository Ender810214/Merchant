  object Qclientes1: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO clientes'
      
        '  (clienteid, nombre, organismoid, provinciaid, telefono, fax, c' +
        'orreo, nit, fecha_acr, tipo_cliente, direccion, codigo)'
      'VALUES'
      
        '  (:clienteid, :nombre, :organismoid, :provinciaid, :telefono, :' +
        'fax, :correo, :nit, :fecha_acr, :tipo_cliente, :direccion, :codi' +
        'go)')
    SQLDelete.Strings = (
      'DELETE FROM clientes'
      'WHERE'
      '  clienteid = :Old_clienteid')
    SQLUpdate.Strings = (
      'UPDATE clientes'
      'SET'
      
        '  clienteid = :clienteid, nombre = :nombre, organismoid = :organ' +
        'ismoid, provinciaid = :provinciaid, telefono = :telefono, fax = ' +
        ':fax, correo = :correo, nit = :nit, fecha_acr = :fecha_acr, tipo' +
        '_cliente = :tipo_cliente, direccion = :direccion, codigo = :codi' +
        'go'
      'WHERE'
      '  clienteid = :Old_clienteid')
    SQLLock.Strings = (
      'SELECT * FROM clientes'
      'WHERE'
      '  clienteid = :Old_clienteid'
      'FOR UPDATE NOWAIT')
    SQLRefresh.Strings = (
      
        'SELECT clienteid, nombre, organismoid, provinciaid, telefono, fa' +
        'x, correo, nit, fecha_acr, tipo_cliente, direccion, codigo FROM ' +
        'clientes'
      'WHERE'
      '  clienteid = :clienteid')
    SQLRecCount.Strings = (
      'SELECT count(*) FROM ('
      'SELECT * FROM clientes'
      ''
      ') t')
    Connection = Fmain.UniConnection1
    SQL.Strings = (
      'select * from clientes')
    Left = 368
    Top = 337
